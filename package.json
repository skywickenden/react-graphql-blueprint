{
  "name": "react-client-blueprint",
  "version": "1.0.0",
  "description": "A blueprint for React with Webpack and Babel. React router, Linaria for CSS and Jest with React Testing Library for tests.",
  "scripts": {
    "test": "jest --watchAll",
    "start": "webpack-dev-server --mode development --hot --host 0.0.0.0",
    "getschema": "graphql get-schema --project client --watch",
    "relay": "relay-compiler --src ./src --schema ./schema.graphql"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skywickenden/react-client-blueprint.git"
  },
  "author": "Sky Wickenden",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/skywickenden/react-client-blueprint/issues"
  },
  "homepage": "https://github.com/skywickenden/react-client-blueprint#readme",
  "dependencies": {
    "linaria": "^1.3.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-relay": "^5.0.0",
    "react-router-dom": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@testing-library/react": "^8.0.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-relay": "^5.0.0",
    "core-js": "^2.6.9",
    "css-loader": "^2.1.1",
    "graphql": "^14.3.1",
    "graphql-query-test-mock": "^0.11.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "jest-dom": "^3.5.0",
    "mini-css-extract-plugin": "^0.7.0",
    "nock": "^10.0.6",
    "relay-compiler": "^5.0.0",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.3",
    "webpack-dev-server": "^3.7.1"
  }
}
